    "vuedraggable": "^2.23.1",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@fiahfy/icns-convert": "^0.0.6",
    "@fiahfy/ico-convert": "^0.0.5",
    "cross-env": "^6.0.0",
    "electron": "^6.0.10",
    "electron-builder": "^21.2.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.2.3",
    "husky": "^3.0.5",
